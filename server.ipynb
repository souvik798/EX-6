    "s=socket.socket()\n",
    "s.connect(('localhost',8000))\n",
    "while True:\n",
    "    ip=input(\"Enter the website you want to ping \")\n",
    "    s.send(ip.encode())\n",
    "    print(s.recv(1024).decode())"
